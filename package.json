{
  "name": "@coconut-xr/input",
  "version": "0.0.1",
  "homepage": "https://coconut-xr.github.io/input",
  "license": "SEE LICENSE IN LICENSE",
  "description": "input fields in 3D UIs for @coconut-xr/koestlich",
  "author": "Bela Bohlender",
  "keywords": [
    "r3f",
    "three.js",
    "userinterface",
    "react",
    "input",
    "textfield",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:coconut-xr/input.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "run-s check:prettier check:eslint",
    "check:prettier": "prettier --check src/**/*.ts",
    "check:eslint": "eslint src",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:prettier": "prettier --write src/**/*.ts",
    "fix:eslint": "eslint src --fix"
  },
  "devDependencies": {
    "@react-three/fiber": "^8.13.5",
    "@types/react": "^18.2.16",
    "@types/three": "^0.154.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "three": "^0.154.0",
    "typescript": "^5.1.6",
    "@coconut-xr/koestlich": "0.3.2"
  },
  "peerDependencies": {
    "@coconut-xr/koestlich": ">=0.3.0"
  }
}
